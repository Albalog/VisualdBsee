.do while oo_obj->obj+"G" == oo_prn->obj+oo_prn->rec_id .AND. !oo_prn->(eof())
    .*
    .* check se  l'ultimo raggruppamento
    .*
    .oo_prn->( dbSkip( 1 ) )
    .if oo_prn->(eof()) .OR. (oo_obj->obj+"G" != (oo_prn->obj+oo_prn->rec_id))
        .lTLastGrp := .T.
    .endif
    .oo_prn->( dbSkip( -1 ) )
    .*
.*   ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
