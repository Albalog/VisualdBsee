.parameter TmpOpe
.* ----------------------------------------------------------------------------
.* TEMPLATE     : GENDOM
.* RELEASE      : 4.0
.* VERSION      : d
.* DESCRIZIONE  : Genera il gruppo di oggetti selezionato
.* ----------------------------------------------------------------------------
.msg [Operazione in corso - Attendere Prego ! ]
.M->MStOpe := TmpOpe
.M->aDom   := {}
.M->aDomR  := {}
.M->aObj   := {}
.M->aObjR  := {}
.M->aObjT  := {}
.do while TmpOpe!="EOP"
.do case
 .case TmpOpe == "IN2"
       .msg [Caricamento elenco OGGETTI della gestione in corso - Attendere Prego ! ]
       .M->bDis:={||if(!empty(oo_obj->FLGUPD),'* ','  ')+PadR(trim(oo_obj->prg)+".prg",15)+" - "+tObjTemplate("tmp")+" - "+Subs(oo_obj->objdes,1,30)}
       .aadd( m->aObj , eval(M->bDis))
       .aadd( m->aObjR, oo_obj->(recno()) )
       .if !empty(oo_obj->OBJ2OBJ)  ./  esiste un oggetto associato !
           .m->nRecPos := oo_obj->(recno())
           .oo_obj->(dbSeek(oo_obj->OBJ2OBJ))
           .aadd( m->aObj , eval(M->bDis))
           .aadd( m->aObjR, oo_obj->(recno()) )
           oo_obj->(dbGoto(m->nRecPos))
       .endif
       .M->TmpOpe:="OBJ"
       .*
 .case TmpOpe == "OBJ"
       .usermsg [<Ù=Genera oggetti marcati - Ctrl+Barra=Marca o smarca l'oggetto]
       .dBwar(0,0,0,0,[³SELEZIONE OGGETTO³],[],aObj,aObjT:={},1,0,[dbwar],NIL, [ret-esc],[s],[s],0)
       .if (M->Act!="esc") .and. (!Empty(aObjT))
          .M->nCnt := 1
          .do while nCnt <= len(aObjT)
              .if aObjT[nCnt] != 0
                  .nObjR := aObjR[ aObjT[nCnt] ]
                  .oo_obj->(DbGoto(nObjR))
                  .if oo_obj->(dbNet("rec"))
                     .dd_dbf->(DbSetorder(1))
                     .dd_dbf->(DbSeek(oo_obj->dbf))
.do ÛtObjTemplate("tmp")Û
                     .oo_obj->(DbGoto(nObjR))
                     .oo_obj->PRGDATE := date()
                     .oo_obj->PRGTIME := dfTimeStd()
                     .oo_obj->FLGUPD  := ""
                     .oo_obj->(dbNet("url"))

                     .if oo_obj->WDO=="mke"
                         .oo_obj->(DbGotop())
                         .do while !oo_obj->(EOF())
                             .if (oo_obj->TYP $ "lib-ext-spc")
                                 .if oo_obj->(dbNet("rec"))
                                     .oo_obj->PRGDATE := date()
                                     .oo_obj->PRGTIME := dfTimeStd()
                                     .oo_obj->FLGUPD  := ""
                                     .oo_obj->(dbNet("url"))
                                 .endif
                             .endif
                             .oo_obj->(dbSkip())
                         .enddo
                         .oo_obj->(DbGoto( nObjR ))
                     .endif
                     .tUpgradeEnv()

                  .endif
              .endif
              .M->nCnt := M->nCnt + 1
          .enddo
       .endif
       .M->TmpOpe:= "EOP"
.endcase
.enddo
